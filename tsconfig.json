{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/espaceMembre/page.js", ".next/types/app/page.js", ".next/types/app/layout.js", ".next/types/app/responsable/page.js", ".next/types/app/espaceResponsable/ressources/page.js", ".next/types/app/espaceResponsable/page.js", "app/espaceAdmin/Layout.tsx", "app/espaceAdmin/page.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
